@model List<Mountain_Goats_Bike_App.Models.Orders>

    <h1>Orders (First 30)</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Order ID</th>
                <th>Customer ID</th>
                <th>Order Status</th>
                <th>Order Date</th>
                <th>Required Date</th>
                <th>Shipped Date</th>
                <th>Store ID</th>
                <th>Staff ID</th>
            </tr>
        </thead>
        <tbody>


        @foreach (var orders in Model)
        {
                    <tr>
                        <td>@orders.Order_id</td>
                        <td>@orders.Customer_id</td>
                        <td>@orders.Order_status</td>
                        <td>@orders.Order_date</td>
                        <td>@orders.Required_date</td>
                        <td>@orders.Shipped_date</td>
                        <td>@orders.Store_id</td>
                        <td>@orders.Staff_id</td>
                    </tr>
        }
        </tbody>
    </table>
